{
  "name": "version-print",
  "version": "0.0.0",
  "description": "make software versions text for report",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "elzup/version-print",
  "author": "elzup <guild0105@gmail.com> (elzup.com)",
  "bin": "dist/cli.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "cli": "babel-node src/cli.js",
    "build": "babel src --out-dir dist && npm run prepack:flow",
    "deploy": "npm run build && yarn publish",
    "prepack:flow": "flow-copy-source src dist",
    "add-contributor": "all-contributors add",
    "precommit": "yarn flow && lint-staged",
    "fmt": "prettier --write '**/*.{js,json,md}'",
    "flow": "flow",
    "lint": "eslint .",
    "test": "jest src",
    "test:cov": "jest src --coverage --ci --runInBand",
    "test:ci": "yarn flow && yarn lint && yarn test:cov"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli-app",
    "cli",
    "version",
    "report",
    "issue"
  ],
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.54",
    "@babel/core": "^7.0.0-beta.54",
    "@babel/node": "^7.0.0-beta.54",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/preset-flow": "^7.0.0-beta.54",
    "all-contributors-cli": "^5.4.0",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^23.0.1",
    "eslint": "^5.6.0",
    "eslint-config-precure": "^2.7.0",
    "flow-bin": "^0.81.0",
    "flow-copy-source": "^2.0.0",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "inquirer": "^6.2.0",
    "meow": "^3.7.0"
  }
}
